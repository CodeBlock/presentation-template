\ProvidesPackage{beamercolorthemeRBE}
  [2016/06/25 v2.0 beamer 'RBE' color theme]

\mode<presentation>

\definecolor{RHRed}{RGB}{204,0,0}
\definecolor{RHRedDarker}{RGB}{120,31,28}
\definecolor{RHRedEvenDarker}{HTML}{333333}
\definecolor{NormalText}{RGB}{64,64,64}
\definecolor{AlmostWhite}{HTML}{E3E2E1}
\definecolor{pomegranate}{HTML}{003954}
\definecolor{belize}{HTML}{82CDFF}
\definecolor{midnight}{HTML}{1B242D}


\setbeamercolor{normal text}{fg=NormalText}
\setbeamercolor{alerted text}{fg=RHRed}
\setbeamercolor{example text}{fg=NormalText}
\setbeamercolor{structure}{fg=NormalText}

\setbeamercolor{background canvas}{bg=AlmostWhite}

\setbeamercolor{author}{fg=NormalText}
\setbeamercolor{date}{fg=NormalText}
\setbeamercolor{framesubtitle}{fg=NormalText}

\setbeamercolor{title}{fg=midnight}
\setbeamercolor{frametitle}{fg=midnight}
\setbeamercolor{normal text}{fg=midnight}
\setbeamercolor{itemize item}{fg=midnight}
\setbeamercolor{enumerate item}{fg=midnight}

\mode<all>
